<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.avnis.ontime">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:noHistory="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Subject" />
        <activity android:name=".TimeTable" />
        <activity android:name=".Tuesday" />
        <activity android:name=".Wednesday" />
        <activity android:name=".Thursday" />
        <activity android:name=".Friday" />
        <activity android:name=".Saturday" />
        <activity android:name=".Sunday" />
        <activity android:name=".Monday" />
        <activity android:name=".RemoveSubject" />
        <activity android:name=".MondayUpdate" />
        <activity android:name=".TuesdayUpdate" />
        <activity android:name=".WedenesdayUpdate" />
        <activity android:name=".WednesdayUpdate" />
        <activity android:name=".ThursdayUpdate" />
        <activity android:name=".FridayUpdate" />
        <activity android:name=".SaturdayUpdate" />
        <activity android:name=".SundayUpdate" />
        <activity android:name=".AttendaceInfo" />
        <activity android:name=".PrevEntry" />
        <activity android:name=".AboutUs" />
        <activity android:name=".PresentActivity" />
        <activity android:name=".AbsentActivity" />
        <activity android:name=".CancelActivity" />

        <receiver
            android:name=".AlarmReceiver"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.example.Action_SNOOZE" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".NotificationAlarmReceiver"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.example.Action_SNOOZE" />
            </intent-filter>
        </receiver>

        <activity android:name=".NotificationHandler" />
        <activity android:name=".Detail"></activity>
    </application>

</manifest>